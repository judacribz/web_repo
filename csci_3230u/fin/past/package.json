    "mkdirp": "^0.5.1",
    "mongoose": "5.8.11",
    "node-uuid": "1.4.7",
    "pug": "^2.0.0-beta6"
  },
  "devDependencies": {
    "nodemon": "^1.17.3"
  }
}
